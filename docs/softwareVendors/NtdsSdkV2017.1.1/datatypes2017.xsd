					<xs:enumeration value="6">
						<xs:annotation>
							<xs:documentation>Protective Service Occupations</xs:documentation>
						</xs:annotation>
					</xs:enumeration>
					<xs:enumeration value="7">
						<xs:annotation>
							<xs:documentation>Building and Grounds Cleaning and Maintenance</xs:documentation>
